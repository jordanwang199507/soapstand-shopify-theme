<html>
<head>
    {{content_for_header}}
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="shortcut icon" href="{{ favicon.ico | asset_url }}" type="image/png"/>

    {{'style.css' | asset_url | stylesheet_tag}}
    <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@100;200;400;700&display=swap" rel="stylesheet">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.2.0/css/all.min.css" rel="stylesheet">
</head>
<body>
    {% comment %} <div class="landing-page">
        {% render 'header' %}
        {{content_for_layout}}
    </div> {% endcomment %}
    {% comment %} {% section 'header' %} {% endcomment %}
    {% section 'header' %}
    {{content_for_layout}}
    {% section 'footer' %}
    {{ 'script.js' | asset_url | script_tag}}

</body>
</html>
